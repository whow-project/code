@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms:	<http://purl.org/dc/terms/> .
@prefix owl:	<http://www.w3.org/2002/07/owl#> .
dcterms:modified	rdf:type	owl:AnnotationProperty .
dcterms:description	rdf:type	owl:AnnotationProperty .
dcterms:issued	rdf:type	owl:AnnotationProperty .
dcterms:title	rdf:type	owl:AnnotationProperty .
@prefix skos:	<http://www.w3.org/2004/02/skos/core#> .
skos:Concept	rdf:type	owl:Class .
@prefix ns4:	<https://w3id.org/italia/env/onto/top/> .
ns4:Organisation	rdf:type	owl:Class .
@prefix ns5:	<https://w3id.org/whow/onto/water-monitoring/> .
ns5:ChemicalSubstance	rdf:type	owl:Class .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
ns5:ChemicalSubstance	rdfs:subClassOf	ns5:WaterObservablePropertyObject ;
	rdfs:label	"Chemical Substance"@en .
@prefix ns7:	<https://w3id.org/whow/onto/> .
ns5:ChemicalSubstance	rdfs:isDefinedBy	ns7:water-monitoring ;
	rdfs:comment	"This class represents the chemical substances. A controlled vocabulary can be constructed for this class based on the CAS numbering of the chemical substances."@en ;
	owl:disjointWith	ns5:RadioactivityObject .
@prefix ns8:	<https://w3id.org/whow/onto/health-monitoring/> .
ns8:Disease	rdf:type	owl:Class ;
	rdfs:subClassOf	ns4:Concept ;
	rdfs:label	"Disease"@en ,
		"Malattia"@it ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"The class that represents the disease (e.g., COVID-19)."@en ,
		"La classe che rappresenta una malattia (e.g., COVID-19, la varicella, etc.)"@it ;
	owl:versionInfo	"stable"@en ,
		"stabile"@it .
ns8:DiseaseType	rdf:type	owl:Class ;
	rdfs:subClassOf	ns4:Concept ,
		ns4:Characteristic ;
	rdfs:label	"Tipo di Malattia"@it ,
		"Disease Type"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"La classe dei tipi di malattia."@it ,
		"The class of the types of diseases"@en ;
	owl:versionInfo	"stable"@en ,
		"stabile"@it .
ns8:narrower	rdf:type	owl:ObjectProperty ;
	rdfs:label	"narrower"@en ,
		"sotto insieme"@it ;
	rdfs:domain	ns8:DiseaseType ;
	rdfs:range	ns8:DiseaseType ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Propriet\u00E0 che consente di modellare una tassonomia di tipi di malattie."@it ,
		"Property used to create a taxonomy of different types of diseases."@en ;
	owl:versionInfo	"stable"@en ,
		"stabile"@it .
@prefix ns9:	<https://w3id.org/whow/onto/water-indicator/> .
ns9:WaterIndicator	rdf:type	owl:Class .
@prefix ns10:	<https://w3id.org/italia/onto/Indicator/> .
ns9:WaterIndicator	rdfs:subClassOf	ns10:Indicator ,
		_:b10024 ,
		_:b10025 ;
	rdfs:label	"Water Indicator"@en ;
	rdfs:isDefinedBy	ns7:water-indicator ;
	rdfs:comment	"The class of the different types of indicator defined for calculate the quality of the water. Examples are LTLeco, LIMeco, etc."@en .
@prefix dc:	<http://purl.org/dc/elements/1.1/> .
dc:description	rdf:type	owl:AnnotationProperty .
@prefix ns12:	<https://w3id.org/italia/env/onto/inspire-mf/> .
ns12:Indicator	rdf:type	owl:Class .
ns12:isIndicatorOf	rdf:type	owl:ObjectProperty .
ns4:Characteristic	rdf:type	owl:Class .
ns4:Concept	rdf:type	owl:Class .
ns4:Description	rdf:type	owl:Class .
ns4:Entity	rdf:type	owl:Class .
ns4:Eventuality	rdf:type	owl:Class .
ns4:Location	rdf:type	owl:Class .
ns4:Value	rdf:type	owl:Class .
ns4:atTime	rdf:type	owl:ObjectProperty .
ns4:isClassifiedBy	rdf:type	owl:ObjectProperty .
ns7:health-monitoring	rdf:type	owl:Ontology .
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#> .
ns7:health-monitoring	dcterms:modified	"2023-06-06"^^xsd:date ;
	rdfs:label	"Health Care Indicator Monitoring Ontology"@en ,
		"Ontologia di Monitoraggio di Indicatori della Salute"@it .
@prefix ns14:	<https://w3id.org/italia/env/onto/> .
ns7:health-monitoring	owl:imports	ns14:top ;
	owl:versionInfo	"Version 0.3 - June 2023 - modified some elements of the ontology (introduction of the property hasScope for the Cohort and related elements, revision of the organisations involved in the computation of some indicators)"@en ;
	dcterms:description	"Questa \u00E8 l'ontologia del progetto WHOW utilizzata per rappresentare un insieme di indicatori utilizzati nel settore sanitario della regione Lombardia per monitorare alcuni parametri sanitari (ad esempio, la diffusione di malattie infettive, la distribuzione di farmaci, l'andamento delle cause di morte e l'accesso a specifiche strutture di ricovero). L'ontologia \u00E8 generale per modellare indicatori in ambito sanitario e non \u00E8 strettamente dipendente dai dataset (aperti) della regione Lombardia."@it ,
		"This is the ontology of the WHOW project that used to represent a set of indicators used in the health sector of Lombardy region to monitor some health parameters (e.g., spread of infectious diseases, distribution of drugs, causes of deaths and access to specific inpatient facilities). The ontology is general to model indicators in the health domain and not strictly dependent on Lombarday region (open) datasets."@en ;
	dcterms:issued	"2022-10-03"^^xsd:date ;
	dcterms:title	"Ontologia di Monitoraggio di Indicatori della Salute"@it ,
		"Healthcare Indicator Monitoring Ontology"@en ;
	owl:versionIRI	<urn:absolute:https//w3id.org/whow/onto/health-monitoring/0.3> ;
	dc:description	<https://raw.githubusercontent.com/whow-project/semantic-assets/main/ontologies/graphical-representation-ontologies/health-monitoring.png> .
ns8:CauseOfDeath	rdf:type	owl:Class ;
	rdfs:subClassOf	_:b10007 ,
		ns4:Concept ;
	rdfs:label	"Cause of Death"@en ,
		"Causa di Morte"@it ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"La classe di tutte quelle circostanze/situazioni/condizioni morbose che portano direttamente alla morte della persona."@it ,
		"The class of all those circumstances/situations/pathological conditions that lead directly to a person's death."@en ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:ClinicalCohort	rdf:type	owl:Class ;
	rdfs:subClassOf	ns8:Population ,
		_:b10008 ;
	rdfs:label	"Coorte Clinica"@it ,
		"Clinical Cohort"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"The class of the sole clinical cohort (actually a subclass of the general population) on which the health indicator is calculated. It is characterised by criteria or parameters for its composition (e.g. gender, age) and is associated with a generic scope that varies according to the type of indicator being calculated."@en ,
		"L'insieme della sola popolazione clinica (\u00E8 infatti sotto classe della popolazione generale) che rappresenta la popolazione su cui si calcola l'indicatore di salute. Essa \u00E8 caratterizzata da criteri o parametri per la sua composizione (e.g., sesso, et\u00E0) ed \u00E8 associata a un generico ambito che varia a seconda del tipo di indicatore che viene calcolato."@it ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:CohortCriteriaDescription	rdf:type	owl:Class ;
	rdfs:subClassOf	_:b10009 ,
		_:b10010 ,
		ns4:Description ;
	rdfs:label	"Cohort Criteria Description"@en ,
		"Descrizione dei Criteri per Dimensione Popolazione Clinica"@it ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"La classe rappresenta una reificazione e implementa il design pattern dell'ontologia Description. Grazie a questo pattern \u00E8 possibile descrivere  la dimensione della coorte clinica sulla base di criteri, non sempre noti a priori, e dei relativi valori."@it ,
		"The class represents a reification and implements the ontology design pattern named Description. With this pattern it is possible to describe the dimension of the clinical cohort on the basis of criteria, not always known a priori, and their values,"@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:Criterion	rdf:type	owl:Class ;
	rdfs:subClassOf	ns4:Characteristic ;
	rdfs:label	"Criterio"@it ,
		"Criterion"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"La classe che rappresenta criteri. Nello specifico contesto di questa ontologia sono i criteri attraverso cui viene descritta la dimensione della popolazione clinica che concorre al calcolo dell'indicatore di salute. Esempi di criteri includono: et\u00E0. sesso, anno di inizio sintomi per una malattia infettiva, ecc."@it ,
		"The class representing criteria. In the specific context of this ontology, these are the criteria by which the  dimension of the clinical population contributing to the calculation of the health indicator is described. Examples of criteria include: age. gender, year of start of symptoms for an infectious disease, etc."@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:DrugDistributionIndicatorCalculation	rdf:type	owl:Class ;
	rdfs:subClassOf	ns8:HealthcareIndicatorCalculation ;
	rdfs:label	"Calcolo dell'Indicatore Distribuzione Farmaci"@it ,
		"Drug Distribution Indicator Calculation"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"The class of the drug distribution indicator calculation. This class is equivalent to the 'health indicator calculation when the health indicator is 'drug distribution'.'"@en ,
		"La classe del calcolo dell'indicatore relativo alla distribuzione dei farmaci. Questa classe \u00E8 equivalente al 'calcolo dell'indicatore di salute quando l'indicatore sanitario \u00E8  'distribuzione farmaci'.'"@it ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:DrugType	rdf:type	owl:Class ;
	rdfs:subClassOf	_:b10011 ,
		_:b10012 ,
		ns4:Concept ;
	rdfs:label	"Tipo di Farmaco"@it ,
		"Drug Type"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"The class of categories of drugs consumed by people."@en ,
		"La classe delle tipologie di farmaci assunte da persone."@it ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:GroupCauseOfDeath	rdf:type	owl:Class ;
	rdfs:subClassOf	ns4:Concept ;
	rdfs:label	"Gruppo di cause di morte"@it ,
		"Group pf Causes of Death"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Classificazione formata da macro-categorie di cause che portano alla morte di persone."@it ,
		"Classification consisting of macro-categories of causes leading to the death of people."@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:HealthConditionIndicatorCalculation	rdf:type	owl:Class ;
	rdfs:subClassOf	ns8:HealthcareIndicatorCalculation ;
	rdfs:label	"Calcolo dell'Indicatore Condizioni di Salute Generali"@it ,
		"Health Condition Indicator Calculation"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"The class of the indicator calculation related to the general health condition of the population. This class is equivalent to the 'health indicator calculation when the health indicator is 'general health conditions'.'"@en ,
		"La classe del calcolo dell'indicatore relativo alle condizioni generali di salute della popolazione. Questa classe \u00E8 equivalente al 'calcolo dell'indicatore di salute quando l'indicatore sanitario \u00E8  'condizioni di salute generali'.'"@it ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:HealthOrganisation	rdf:type	owl:Class ;
	rdfs:subClassOf	ns4:Organisation ;
	rdfs:label	"Organizzazione Sanitaria"@en ,
		"Health Organisation"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Una generica organizzazione che opera nel contesto sanitario. Questa classe include organizzazioni tipo le ASL o le strutture di ricovero."@it ,
		"A generic organisation operating in the healthcare context. This class includes organisations such as ASLs or in-patient facilities."@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:HealthcareAuthority	rdf:type	owl:Class ;
	rdfs:subClassOf	_:b10013 ,
		ns8:HealthOrganisation ;
	rdfs:label	"Agenzia/Azienda Sanitaria"@it ,
		"Healthcare Authority"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"The class of specific organisations that usually deal with health care in a territorial context (in the case of the Lombardy Region they are named ATS, in other regions they are called ASL or AUSL). These typically have the following functions\n1) negotiation and purchase of health and socio-health services from accredited facilities;\n2) governance of the person's care pathway throughout the network of health, social and health services\n3) governance of primary and contracted primary care;\n4) governance and promotion of health education, prevention, care, treatment and rehabilitation programmes;\n5) promotion of medical and veterinary food safety;\n6) prevention and control of health in living and working environments, including periodic plant and technological control;\n7) veterinary public health\n8) implementation of regional guidelines and monitoring of expenditure on pharmaceuticals, dietetics and prosthetics;\n9) supervision and control of healthcare, social and health facilities and units."@en ,
		"La classe delle organizzazioni specifiche che di solito in ambito territoriale si occupano di assistenza sanitaria (nel caso di Regione Lombardia sono le ATS, in altre regioni si chiamano ASL o AUSL). Queste hanno tipicamente le seguenti funzioni:\n1) negoziazione e acquisto delle prestazioni sanitarie e sociosanitarie dalle strutture accreditate;\n2) governo del percorso di presa in carico della persona in tutta la rete dei servizi sanitari, sociosanitari e sociali;\n3) governo dell\u2019assistenza primaria e del convenzionamento delle cure primarie;\n4) governo e promozione dei programmi di educazione alla salute, prevenzione, assistenza, cura e riabilitazione;\n5) promozione della sicurezza alimentare medica e medica veterinaria;\n6) prevenzione e controllo della salute negli ambienti di vita e di lavoro, compreso il controllo periodico impiantistico e tecnologico;\n7) sanit\u00E0 pubblica veterinaria;\n8) attuazione degli indirizzi regionali e monitoraggio della spesa in materia di farmaceutica, dietetica e protesica;\n9) vigilanza e controllo sulle strutture e sulle unit\u00E0 d\u2019offerta sanitarie, sociosanitarie e sociali."@it ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en ;
	owl:disjointWith	ns8:InpatientFacility .
ns8:HealthcareIndicator	rdf:type	owl:Class ;
	rdfs:subClassOf	_:b10014 ,
		ns4:Entity ;
	rdfs:label	"Indicatore di Salute"@it ,
		"Health Care Indicator"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"La classe degli indicatori di salute. Utilizzata per poter creare un vocabolario controllato di indicatori in questo settore applicativo."@it ,
		"The class of the health care indicator. It is used to create a controlled list of possible indicators to be used in the health sector."@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:HealthcareIndicatorCalculation	rdf:type	owl:Class ;
	rdfs:subClassOf	_:b10015 ,
		_:b10016 ,
		_:b10017 ,
		_:b10018 ,
		_:b10019 ,
		ns12:Indicator ,
		ns4:Eventuality ;
	rdfs:label	"Healthcare Indicator Calculation"@en ,
		"Calcolo dell'Indicatore di Salute"@it ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"The class that represents the calculation of the health care indicator and thus brings together the indicator being calculated, the value resulting from the calculation, the clinical cohort used according to certain population breakdown criteria and the relevant territorial area."@en ,
		"La classe che rappresenta il calcolo dell'indicatore di salute e che quindi mette insieme l'indicatore che viene calcolato, il valore risultante dal calcolo, la coorte clinica utilizzata in base a determinati criteri di suddivisione della popolazione e l'area territoriale di pertinenza."@it ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:HospitalCareIndicatorCalculation	rdf:type	owl:Class ;
	rdfs:subClassOf	ns8:HealthcareIndicatorCalculation ;
	rdfs:label	"Hospital Care Indicator Calculation"@en ,
		"Calcolo Indicatore Assistenza Ospedaliera"@it ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"The class of the hospital care indicator calculation. This class is equivalent to the 'health indicator calculation when the health indicator is 'hospital care'.'"@en ,
		"La classe del calcolo dell'indicatore relativo all'assistenza ospedaliera. Questa classe \u00E8 equivalente al 'calcolo dell'indicatore di salute quando l'indicatore sanitario \u00E8  'assistenza ospedaliera'.'"@it ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:IndicatorType	rdf:type	owl:Class ;
	rdfs:subClassOf	ns4:Characteristic ,
		ns4:Concept ;
	rdfs:label	"Indicator Type"@en ,
		"Tipo di Indicatore"@it ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"La classe dei diversi tipi di indicatori. Tipicamente nel settore sanitario, i tipi di indicatori sono: indicatore di processo, indicatore di risultato, indicatore di struttura."@it ,
		"The class of the different types of indicators. Typically in the health care sector, types of indicators are: process indicator, outcome indicator, structure indicator."@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:InfectiousDiseaseIndicatorCalculation	rdf:type	owl:Class ;
	rdfs:subClassOf	ns8:HealthcareIndicatorCalculation ;
	rdfs:label	"Calcolo dell'Indicatore Malattie Infettive"@it ,
		"Infectious Disease Indicator Calculation"@en ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"La classe del calcolo dell'indicatore relativo alla distribuzione delle malattie infettive. Questa classe \u00E8 equivalente al 'calcolo dell'indicatore di salute quando l'indicatore sanitario \u00E8  'diffusione delle malattie infettive'.'"@en ,
		"The class of the specific calculation related to infectious diseases distribution. This class is equivalent to the Healthcare Indicator Calculation when the Healthcare Indicator is diffusion of infectious diseases."@en ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:InpatientFacility	rdf:type	owl:Class ;
	rdfs:subClassOf	ns8:HealthOrganisation ;
	rdfs:label	"Inpatient Facility"@en ,
		"Struttura di Ricovero e Cura"@it ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Hospital care guarantees admission to accredited public and private hospitals for the diagnosis and treatment of acute or long-term illnesses that cannot be dealt with on an outpatient or home basis. The Ministry of Health, as part of the Health Information System\nHealth Information System collects information on public and private hospitalisation facilities and their activities (provenance: Italian Ministry of Health)"@en ,
		"L\u2019assistenza ospedaliera garantisce il ricovero presso gli ospedali pubblici e privati accreditati per la diagnosi e la cura delle malattie acute o di lunga durata che non possono essere affrontate ambulatorialmente o a domicilio. Il Ministero della Salute, nell\u2019ambito del Sistema Informativo\nSanitario raccoglie informazioni sulla struttura di ricovero pubblica e privata e relative attivit\u00E0 della struttura (fonte Ministero della salute)"@it ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:Population	rdf:type	owl:Class ;
	rdfs:subClassOf	_:b10020 ,
		ns4:Entity ;
	rdfs:label	"Population"@en ,
		"Popolazione"@it ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"La classe che rappresenta la popolazione (di una regione in questa ontologia) totale, utilizzata nel calcolo degli indicatori di salute."@it ,
		"The class of the entire popolation considered when computing specific healthcare indicator calculation operations"@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:affectedBy	rdf:type	owl:ObjectProperty ;
	rdfs:subPropertyOf	ns8:hasScope ;
	rdfs:label	"affetta da"@it ,
		"affected by"@en ;
	rdfs:domain	ns8:ClinicalCohort ;
	rdfs:range	_:b10003 ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Propriet\u00E0 che consente di collegare la coorte clinica con la malattia  di cui sono affetti."@it ,
		"Property that links the clinical cohort with the disease they are affected by."@en ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:assumed	rdf:type	owl:ObjectProperty ;
	rdfs:subPropertyOf	ns8:hasScope ;
	rdfs:label	"assumed"@en ,
		"assunto"@it ;
	rdfs:domain	ns8:ClinicalCohort ;
	rdfs:range	ns8:DrugType ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Propriet\u00E0 che specializza la propriet\u00E0 ha ambito usata per definire la coorte clinica che ha assunto specifiche tipologie di farmaci, quando l'indicatore da calcolare riguarda il tasso di distribuzione di farmaci."@it ,
		"Property that specialises has scope, used to define the clinical cohort that assumed specific types of drugs, when the indicator to be calculated concerns the drug distribution rate."@en ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:diedFrom	rdf:type	owl:ObjectProperty ;
	rdfs:subPropertyOf	ns8:hasScope ;
	rdfs:label	"died from"@en ,
		"morte per"@it ;
	rdfs:domain	ns8:ClinicalCohort ;
	rdfs:range	ns8:CauseOfDeath ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Propriet\u00E0 che specializza la propriet\u00E0 ha ambito usata per definire la coorte clinica deceduta per  specifiche cause di morte, quando l'indicatore da calcolare riguarda il numero dei decessi per specifiche cause di morte."@it ,
		"Property that specialises the has scope properties in order to define the clinical cohort died from causes of death when the indicator to be computed is about  the number of deaths from specific causes of death."@en ;
	owl:versionInfo	"unstable"@en ,
		"provvisoria"@it .
ns8:doneForHCIndicator	rdf:type	owl:ObjectProperty ;
	rdfs:label	"done for healthcare indicator"@en ,
		"fatto per indicatore sanitario"@it ;
	rdfs:domain	ns8:HealthcareIndicatorCalculation ;
	rdfs:range	ns8:HealthcareIndicator ;
	rdfs:comment	"Property that links the calculation of an indicator with the healthcare indicator itself."@en ,
		"Propriet\u00E0 che collega il calcolo dell'indicatore, con l'indicatore stesso."@it ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:hasCohortCriteriaDescription	rdf:type	owl:ObjectProperty ;
	rdfs:label	"ha descrizione criteri coorte clinica"@it ,
		"has clinical cohort criteria description"@en ;
	rdfs:domain	ns8:ClinicalCohort ;
	rdfs:range	ns8:CohortCriteriaDescription ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Propriet\u00E0 che lega la coorte clinica con la descrizioni dei criteri che ne determinano la sua dimensione."@it ,
		"Property that links the clinical cohort with the description of the criteria that determine the dimension of the cohort."@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:hasCriterion	rdf:type	owl:ObjectProperty ;
	rdfs:label	"has criterion"@en ,
		"ha criterio"@it ;
	rdfs:domain	ns8:CohortCriteriaDescription ;
	rdfs:range	ns8:Criterion ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Propriet\u00E0 che collega la descrizione dei criteri per la dimensione della coorte clinica a ogni singolo criterio."@it ,
		"Property that links the description of the criteria for the dimension of the clinical cohort to each single criterion."@en ;
	owl:versionInfo	"stabile"@it ,
		"stable"@en .
ns8:hasHealthcareIndicatorValue	rdf:type	owl:ObjectProperty ;
	rdfs:label	"has healthcare indicator value"@en ,
		"ha valore indicatore di salute"@it ;
	rdfs:domain	ns8:HealthcareIndicatorCalculation ;
	rdfs:range	ns4:Value ;
	rdfs:isDefinedBy	ns7:health-monitoring ;
	rdfs:comment	"Propriet\u00E0 utilizzata per collegare il calcolo dell'indicatore di salute con il relativo valore."@it ,
		"Property used to link the healthcare indicator calculation with the related value."@en ;

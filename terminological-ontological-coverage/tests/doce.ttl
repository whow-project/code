@prefix : <http://purl.org/nemo/doce#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix wgs: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gufo: <http://purl.org/nemo/gufo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@base <http://purl.org/nemo/doce#> .

<http://purl.org/nemo/doce#> rdf:type owl:Ontology ;
                              owl:versionIRI <http://purl.org/nemo/doce#/2022-02-23> ;
                              dc:creator "Almeida, João Paulo A." ;
                              dc:date "2023-04-25"@en ;
                              dc:title "The doce water quality ontology"@en ;
                              dct:license <https://creativecommons.org/licenses/by/4.0/legalcode> ;
                              vann:preferredNamespaceUri "http://purl.org/nemo/doce#"^^xsd:anyURI ;
                              rdfs:comment """This ontology enables the sharing of water quality data. 

Based partly on P. M. C. Campos, “Designing a Network of Reference Ontologies for the Integration of Water Quality Data,” Master Thesis, Universidade Federal do Espírito Santo, 2019. 

See <https://nemo.inf.ufes.br/en/projetos/integradoce/> for background on the project and publications. 

The doce water quality ontology specializes the gUFO implementation of UFO. See <http://purl.org/nemo/doc/gufo> for reference documentation.

Cite this work as: 

J. P. A. Almeida, \"The doce water quality ontology\", http://purl.org/nemo/doc/doce

For the source repository, see: <https://github.com/nemo-ufes/doce>""" ;
                              rdfs:label "The doce water quality ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://purl.org/nemo/gufo#partitions
gufo:partitions rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/nemo/doce#delegated
:delegated rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf gufo:participatedIn ;
           rdfs:domain :ResearchActivityPrincipal ;
           rdfs:range :ResearchActivity ;
           rdfs:comment "Identifies a doce:ResearchActivity which the doce:ResearchActivityPrincipal delegated."@en ;
           rdfs:label "delegated"@en .


###  http://purl.org/nemo/doce#expressedIn
:expressedIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Measurement ;
             rdfs:range :Unit ;
             rdfs:comment "Identifies a doce:Unit with which the doce:Measurement is expressed."@en ;
             rdfs:label "expressedIn"@en .


###  http://purl.org/nemo/doce#hasApplicableUnit
:hasApplicableUnit rdf:type owl:ObjectProperty ;
                   rdfs:domain :QualityKind ;
                   rdfs:range :Unit ;
                   rdfs:comment """Identifies a doce:Unit which is applicable to the doce:QualityKind.

Corresponds to http://qudt.org/schema/qudt/applicableUnit"""@en ;
                   rdfs:label "hasApplicableUnit"@en .


###  http://purl.org/nemo/doce#hasAssociatedChemicalEntityType
:hasAssociatedChemicalEntityType rdf:type owl:ObjectProperty ,
                                          owl:FunctionalProperty ;
                                 rdfs:domain :ChemicalEntityConcentrationQualityKind ;
                                 rdfs:range :ChemicalEntityType ;
                                 rdfs:comment """Identifies the doce:ChemicalEntityType which is associated to the doce:ChemicalEntityConcentrationQualityKind.

For example, \"Arsenic\" (a doce:ChemicalEntityType) is associated with \"ArsenicConcentration\" (a doce:ChemicalEntityConcentrationQualityKind)."""@en ;
                                 rdfs:label "hasAssociatedChemicalEntityType"@en .


###  http://purl.org/nemo/doce#locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf gufo:hasReifiedQualityValue ;
           rdfs:domain :GeographicallyLocatedEntity ;
           rdfs:range :GeographicPoint ;
           rdfs:comment "Identifies a doce:GeographicPoint for a doce:GeographicallyLocatedEntity."@en ;
           rdfs:label "locatedIn"@en .


###  http://purl.org/nemo/doce#measured
:measured rdf:type owl:ObjectProperty ;
          owl:inverseOf :wasMeasuredIn ;
          rdfs:domain :Measurement ;
          rdfs:range gufo:Object ;
          rdfs:comment "Identifies the gufo:Object measured in a doce:Measurement."@en ;
          rdfs:label "measured"@en .


###  http://purl.org/nemo/doce#measuredQualityKind
:measuredQualityKind rdf:type owl:ObjectProperty ;
                     rdfs:domain :Measurement ;
                     rdfs:range :QualityKind ;
                     rdfs:comment "Identifies the doce:QualityKind that characterizes the measurement."@en ;
                     rdfs:label "measuredQualityKind"@en .


###  http://purl.org/nemo/doce#performed
:performed rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf gufo:participatedIn ;
           rdfs:domain :ResearchActivityAgent ;
           rdfs:range :ResearchActivity ;
           rdfs:comment "Identifies a doce:ResearchActivity which the doce:ResearchActivityAgent performed."@en ;
           rdfs:label "performed"@en .


###  http://purl.org/nemo/doce#represents
:represents rdf:type owl:ObjectProperty ;
            rdfs:domain :Sample ;
            rdfs:range :SampledEntity ;
            rdfs:comment "Identifies the doce:SampleEntity which the doce:Sample represents."@en ;
            rdfs:label "represents"@en .


###  http://purl.org/nemo/doce#wasAdoptedIn
:wasAdoptedIn rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf gufo:participatedIn ;
              rdfs:domain :ResearchActivityProcedure ;
              rdfs:range :ResearchActivity ;
              rdfs:comment "Identifies a doce:ResearchActivity in which the doce:ResearchActivityProcedure was adopted."@en ;
              rdfs:label "wasAdoptedIn"@en .


###  http://purl.org/nemo/doce#wasMeasuredIn
:wasMeasuredIn rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf gufo:participatedIn ;
               rdfs:domain gufo:Object ;
               rdfs:range :Measurement ;
               rdfs:comment "Identifies a doce:Measurement in which the gufo:Object was measured."@en ;
               rdfs:label "wasMeasuredIn"@en .


###  http://purl.org/nemo/doce#wasUsedIn
:wasUsedIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf gufo:participatedIn ;
           rdfs:domain :Device ;
           rdfs:range :ResearchActivity ;
           rdfs:comment "Identifies a doce:ResearchActivity in which the doce:Device was used."@en ;
           rdfs:label "wasUsedIn"@en .


###  http://purl.org/nemo/gufo#hasReifiedQualityValue
gufo:hasReifiedQualityValue rdf:type owl:ObjectProperty .


###  http://purl.org/nemo/gufo#participatedIn
gufo:participatedIn rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
wgs:lat rdf:type owl:DatatypeProperty ;
        rdfs:comment "Latitude expressed in decimal degrees using datum WGS84. In the doce ontology, used to express the latitude of a GeographicPoint with xsd:float values." .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
wgs:long rdf:type owl:DatatypeProperty ;
         rdfs:comment "Longitude expressed in decimal degrees using datum WGS84. In the doce ontology, used to express the longitude of a GeographicPoint with xsd:float values." .


#################################################################
#    Classes
#################################################################

###  http://purl.org/nemo/doce#AbioticEntity
:AbioticEntity rdf:type owl:Class ;
               rdfs:subClassOf :MaterialEntity ;
               owl:disjointWith :BioticEntity ;
               rdfs:comment """A non-living non-biological doce:MaterialEntity.

\"In biology and ecology, abiotic components or abiotic factors are non-living chemical and physical parts of the environment that affect living organisms and the functioning of ecosystems.\"

https://en.wikipedia.org/wiki/Abiotic_component"""@en ;
               rdfs:label "AbioticEntity"@en .


###  http://purl.org/nemo/doce#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf gufo:Object ;
       rdfs:comment "A gufo:Object capable of performing actions, typically a person or an organization."@en ;
       rdfs:label "Agent"@en .


###  http://purl.org/nemo/doce#AmbientTemperature
:AmbientTemperature rdf:type owl:Class ;
                    rdfs:subClassOf :MetereologicalQuality .


###  http://purl.org/nemo/doce#AmmoniacalNitrogenConcentration
:AmmoniacalNitrogenConcentration rdf:type owl:Class ;
                                 rdfs:subClassOf :NonMetalConcentration ;
                                 rdfs:label "AmmoniacalNitrogenConcentration"@en .


###  http://purl.org/nemo/doce#AmountOfWater
:AmountOfWater rdf:type owl:Class ;
               rdfs:subClassOf :AbioticEntity ;
               rdfs:comment """A portion of matter that consists mostly in water.

(Not necessarily a gufo:Quantity, since the identity of a gufo:Quantity is tied to its matter, and amounts of water in this ontology may lose parts while keeping their identity.)"""@en ;
               rdfs:label "AmountOfWater"@en .


###  http://purl.org/nemo/doce#ArtificialGeographicFeature
:ArtificialGeographicFeature rdf:type owl:Class ;
                             rdfs:subClassOf :GeographicFeature ;
                             rdfs:comment """An artificial doce:GeographicFeature.

Examples include \"Colatina\", the \"Candonga Hydroelectric Power Plant Reservoir\"."""@en ;
                             rdfs:label "ArtificialGeographicFeature"@en .


###  http://purl.org/nemo/doce#BiologicalOxygenDemand
:BiologicalOxygenDemand rdf:type owl:Class ;
                        rdfs:subClassOf :PhysicalChemicalQuality ;
                        rdfs:label "BiologicalOxygenDemand"@en .


###  http://purl.org/nemo/doce#BiologicalQualityKind
:BiologicalQualityKind rdf:type owl:Class ;
                       rdfs:subClassOf :WaterQualityKind ;
                       rdfs:comment """A doce:WaterQualityKind for biological qualities in water quality management. 

Examples include \"Toxicity\", \"Concentration of cyanobacteria\"."""@en ;
                       rdfs:label "BiologicalQualityKind"@en .


###  http://purl.org/nemo/doce#BioticEntity
:BioticEntity rdf:type owl:Class ;
              rdfs:subClassOf :MaterialEntity ;
              rdfs:comment """A doce:MaterialEntity that is alive or that originates from living organisms.

\"Biotic components, or biotic factors, can be described as any living component that affects another organism or shapes the ecosystem. This includes both animals that consume other organisms within their ecosystem, and the organism that is being consumed.\"
https://en.wikipedia.org/wiki/Biotic_component"""@en ;
              rdfs:label "BioticEntity"@en .


###  http://purl.org/nemo/doce#ChemicalEntityConcentrationQualityKind
:ChemicalEntityConcentrationQualityKind rdf:type owl:Class ;
                                        rdfs:subClassOf :PhysicalChemicalQualityKind ;
                                        rdfs:comment """A doce:PhysicalChemicalQualityKind concerning the concentration of chemical entities in water. 

Examples include \"ArsenicConcentration\", \"DissolvedOxygenConcentration\"."""@en ;
                                        rdfs:label "ChemicalEntityConcentrationQualityKind"@en .


###  http://purl.org/nemo/doce#ChemicalEntityType
:ChemicalEntityType rdf:type owl:Class ;
                    rdfs:subClassOf gufo:RigidType ;
                    rdfs:comment """A gufo:Type of chemical entity.

Examples include types of atoms, types of molecular entities, types of chemical substances.

Corresponds to <http://purl.obolibrary.org/obo/CHEBI_24431>

See Chemical Entities of Biological Interest <https://www.ebi.ac.uk/ols/ontologies/chebi>"""@en ;
                    rdfs:label "ChemicalEntityType"@en .


###  http://purl.org/nemo/doce#ChlorophyllAConcentration
:ChlorophyllAConcentration rdf:type owl:Class ;
                           rdfs:subClassOf :NonMetalConcentration ;
                           rdfs:label "ChlorophyllAConcentration"@en .


###  http://purl.org/nemo/doce#Concentration
:Concentration rdf:type owl:Class ;
               rdfs:subClassOf :PhysicalChemicalQuality ;
               rdfs:comment "Corresponds to http://qudt.org/vocab/quantitykind/Concentration" ;
               rdfs:label "Concentration"@en .


###  http://purl.org/nemo/doce#Conductivity
:Conductivity rdf:type owl:Class ;
              rdfs:subClassOf :PhysicalChemicalQuality .


###  http://purl.org/nemo/doce#CyanideConcentration
:CyanideConcentration rdf:type owl:Class ;
                      rdfs:subClassOf :NonMetalConcentration ;
                      rdfs:label "CyanideConcentration"@en .


###  http://purl.org/nemo/doce#Device
:Device rdf:type owl:Class ;
        rdfs:subClassOf gufo:Object ;
        rdfs:comment "A gufo:Object that participates in a doce:ResearchActivity as an instrument."@en ;
        rdfs:label "Device"@en .


